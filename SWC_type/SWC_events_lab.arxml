<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///G:/Autosar/Learning_AUTOSAR/schema/AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>Runnable_Lab</SHORT-NAME>
			<ELEMENTS>
			<!-- SR interface -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>Result_IN</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>OUT</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>Input_IN</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Var1</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Var2</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				
				<!-- SWC -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SumAlgo_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Result_PPort</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Runnable_Lab/Result_IN</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Input_RPort</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Runnable_Lab/Input_IN</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>Sw1_SUM</SHORT-NAME>
							
							<EVENTS>
								<INIT-EVENT>
									<SHORT-NAME>IN_event</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/Runnable_Lab/SumAlgo_SWC/Sw1_SUM/SUM_runnable</START-ON-EVENT-REF>
								</INIT-EVENT>
								<TIMING-EVENT>
									<SHORT-NAME>TimeEvent_10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/Runnable_Lab/SumAlgo_SWC/Sw1_SUM/SUM_runnable</START-ON-EVENT-REF>
									<PERIOD>.01</PERIOD>
								</TIMING-EVENT>
								<DATA-RECEIVED-EVENT>
									<SHORT-NAME>Data_Rec_event</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/Runnable_Lab/SumAlgo_SWC/Sw1_SUM/SUM_runnable</START-ON-EVENT-REF>
									<DATA-IREF>
										<TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnable_Lab/Input_IN/Var1</TARGET-DATA-ELEMENT-REF>
									</DATA-IREF>
								</DATA-RECEIVED-EVENT>
								<DATA-WRITE-COMPLETED-EVENT>
									<SHORT-NAME>Data_write_event</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/Runnable_Lab/SumAlgo_SWC/Sw1_SUM/SUM_runnable</START-ON-EVENT-REF>
									<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/Runnable_Lab/SumAlgo_SWC/Sw1_SUM/SUM_runnable/Val1_Data</EVENT-SOURCE-REF>
								</DATA-WRITE-COMPLETED-EVENT>
							</EVENTS>
							
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>SUM_runnable</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Val1_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
												    <PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/Runnable_Lab/SumAlgo_SWC/Input_RPort</PORT-PROTOTYPE-REF>
												    <TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnable_Lab/Input_IN/Var1</TARGET-DATA-PROTOTYPE-REF>												
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
										
										<VARIABLE-ACCESS>
											<SHORT-NAME>Val2_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
												    <PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/Runnable_Lab/SumAlgo_SWC/Input_RPort</PORT-PROTOTYPE-REF>
												    <TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnable_Lab/Input_IN/Var2</TARGET-DATA-PROTOTYPE-REF>												
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>
									
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Out_data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/Runnable_Lab/SumAlgo_SWC/Result_PPort</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Runnable_Lab/Result_IN/OUT</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>
									<SYMBOL>SUM</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>

			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>
